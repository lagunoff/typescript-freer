{
  "compilerOptions": {
    "baseUrl": ".",
    "lib": ["es6", "dom", "es2015"],
    "module": "commonjs",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitUseStrict": true,
    "sourceMap": false,
    "strictNullChecks": true,
    "target": "es5"
  },
  "include": ["./src/*.ts", "./examples/*.ts"],
  "files": ["./src/index.ts"]
}
